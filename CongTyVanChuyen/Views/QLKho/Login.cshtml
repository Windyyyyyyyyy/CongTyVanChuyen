
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html>
<head>
    <title>Simple Login Form</title>
    <meta charset="UTF-8" />
    <link rel="stylesheet" type="text/css" href="~/Content/reset.css">
    <link rel="stylesheet" type="text/css" href="~/Content/structure.css">
</head>
<body>
    <form method="post" action="/QLKho/Login" class="box login">
        <fieldset class="boxBody">
            <label>Username</label>
            <input name="UserNV" type="text" tabindex="1" placeholder="Username" required>
            <label>
                <a href="#" class="rLink" tabindex="5">Forget your password?</a>Password
            </label>
            <input name="PassNV" type="password" tabindex="2" placeholder="Password" required>
        </fieldset>
        <footer>
            <label><input type="checkbox" tabindex="3">Keep me logged in</label>
            <input type="submit" class="btnLogin" value="Login" tabindex="4">
        </footer>
    </form>
</body>
</html>

